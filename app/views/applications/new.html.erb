<%= form_with url: '/applications', local: true do |form| %>
  <%= form.label :name %>
  <%= form.text_field :name %>

  <%= form.label :address %>
  <%= form.text_field :address %>

  <%= form.label :city %>
  <%= form.text_field :city %>

  <%= form.label :state %>
  <%= form.text_field :state %>

  <%= form.label :zip %>
  <%= form.text_field :zip %>

  <%= form.label :description %>
  <%= form.text_area :description %>

  <%= form.submit "Submit" %>
<% end %>
